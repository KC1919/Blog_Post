<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" type="text" name="postTitle" placeholder="Enter"><br>
    <label for="post">Post</label>
    <textarea class="form-control" name="postBody" rows="6" ></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="publish">Publish</button>


</form>

<%- include("partials/footer") -%>
